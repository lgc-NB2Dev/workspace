  "version": "0.2.0",
  "configurations": [
    {
      "name": "Current file",
      "type": "debugpy",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal",
      "justMyCode": false
    },
    {
      "name": "TestNB2",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/private/test-nb2/bot.py",
      "cwd": "${workspaceFolder}/private/test-nb2",
      "console": "integratedTerminal",
      "justMyCode": false,
      "env": {
        // "LOG_LEVEL": "DEBUG"
      }
    },
    {
      "name": "Debug Tests",
      "type": "debugpy",
      "request": "launch",
      "program": "${file}",
      "purpose": ["debug-test"],
      "console": "integratedTerminal",
      "justMyCode": false
    },
    {
      "name": "cookit: Standalone tests",
      "type": "debugpy",
      "request": "launch",
      "module": "tests_standalone",
      "cwd": "${workspaceFolder}/external/cookit",
      "console": "integratedTerminal",
      "justMyCode": false
    },
    {
      "name": "cooaki: Console demo",
      "type": "debugpy",
      "request": "launch",
      "module": "cooaki",
      "cwd": "${workspaceFolder}/external/cooaki",
      "console": "integratedTerminal",
      "justMyCode": false
    }
  ]
}

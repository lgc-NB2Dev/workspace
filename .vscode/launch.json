  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python 调试程序: 当前文件",
      "type": "debugpy",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal",
      "justMyCode": false
    },
    {
      "name": "Run TestNB2",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/private/test-nb2/bot.py",
      "cwd": "${workspaceFolder}/private/test-nb2",
      "console": "integratedTerminal",
      "justMyCode": false,
      "env": {
        // "LOG_LEVEL": "DEBUG"
      }
    },
    {
      "name": "Python: Debug Tests",
      "type": "debugpy",
      "request": "launch",
      "program": "${file}",
      "purpose": ["debug-test"],
      "console": "integratedTerminal",
      "justMyCode": false
    },
    {
      "name": "cookit: Standalone tests",
      "type": "debugpy",
      "request": "launch",
      "module": "tests_standalone",
      "cwd": "${workspaceFolder}/packages/cookit",
      "console": "integratedTerminal",
      "justMyCode": false
    },
    {
      "name": "bawiki-data: Test Global Future Image Crop",
      "type": "debugpy",
      "request": "launch",
      "cwd": "${workspaceFolder}/others/bawiki-data",
      "module": "scripts.tools.future_crop_test",
      "justMyCode": true
    },
    {
      "name": "bawiki-data: Pre Deploy",
      "type": "debugpy",
      "request": "launch",
      "cwd": "${workspaceFolder}/others/bawiki-data",
      "module": "scripts.pre_deploy",
      "justMyCode": true
    },
    {
      "name": "bawiki-data: Pre Deploy: Update Stu Aliases",
      "type": "debugpy",
      "request": "launch",
      "cwd": "${workspaceFolder}/others/bawiki-data",
      "module": "scripts.pre_deploy.update_stu_alias_list",
      "justMyCode": true
    },
    {
      "name": "bawiki-data: Pre Deploy: Gen Gacha Data",
      "type": "debugpy",
      "request": "launch",
      "cwd": "${workspaceFolder}/others/bawiki-data",
      "module": "scripts.pre_deploy.gen_gacha_data",
      "justMyCode": true
    }
  ]
}
